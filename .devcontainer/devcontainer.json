{
  "dockerComposeFile": [
    "compose.yml"
  ],
  "service": "crates-publish-minimal",
  "workspaceFolder": "/workspace",
  "customizations": {
    "vscode": {
      "extensions": [
        "bungcip.better-toml",
        "GitHub.copilot",
        "ms-azuretools.vscode-docker",
        "rust-lang.rust-analyzer",
        "GitHub.vscode-github-actions"
      ],
      "settings": {
        "files.watcherExclude": {
          "**/target/**": true
        }
      }
    }
  }
}